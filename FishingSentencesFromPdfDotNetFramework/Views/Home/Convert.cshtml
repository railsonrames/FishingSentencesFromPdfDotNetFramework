@{
    ViewBag.Title = "Convert";
}

<h2>@ViewBag.Title</h2>
<h3>@ViewBag.Message</h3>

<form action="OnPostConvert" enctype="multipart/form-data" method="post">
    <div class="row">
        <div class="col-md-6">
            <input type="file" name="postedFileBase" />
        </div>
        <div class="col-md-4">
        <input type="submit" class="btn btn-primary" value="Upload" />
            </div>
    </div>
</form>
